<ruleset name="SVG10" display-name="Savage Worlds">
	<datasets>
		<dataset name="skill">
			<data name="name" value-type="string" default="Skill" />
			<data name="rank" value-type="int" />
		</dataset>
		<dataset name="power">
			<data name="name" value-type="string" />
			<data name="trapping" value-type="string" />
			<data name="cost" value-type="int" />
			<data name="range" value-type="int" />
			<data name="effect" value-type="string" />
			<data name="duration" value-type="string" />
		</dataset>
		<dataset name="weapon">
			<data name="name" value-type="string" />
			<data name="range" value-type="string" />
			<data name="rof" value-type="string" />
			<data name="damage" value-type="string" />
			<data name="ap" value-type="string" />
			<data name="wt" value-type="string" />
			<data name="notes" value-type="string" />
		</dataset>
		<dataset name="roll">
			<data name="name" value-type="string" />
			<data name="roll" value-type="string" />
		</dataset>
	</datasets>
	
	<attributes>
		<attribute name="Name" display-name="Name" value-type="string" />
		<attribute name="Player" display-name="Player Name" value-type="string" />
		<attribute name="Origin" display-name="Port of Origin" value-type="string" />
		<attribute name="Rating" display-name="Rating (XP)" value-type="string" />
		<attribute name="Destination" display-name="Destination" value-type="string" />
		<attribute name="Description" display-name="Description" value-type="string" />
		<attribute name="Agility" display-name="Agility" value-type="int" />
		<attribute name="Smarts" display-name="Smarts" value-type="int" />
		<attribute name="Strength" display-name="Strength" value-type="int" />
		<attribute name="Spirit" display-name="Spirit" value-type="int" />
		<attribute name="Vigor" display-name="Vigor" value-type="int" />
		<attribute name="Charisma" display-name="Charisma" value-type="int" />
		<attribute name="Pace" display-name="Pace" value-type="int" />
		<attribute name="Parry" display-name="Parry" value-type="int" />
		<attribute name="Toughness" display-name="Toughness" value-type="int" />
		<attribute name="Fatigue" display-name="Fatigue" value-type="int" />
		<attribute name="Wounds" display-name="Wounds" value-type="int" />
		<attribute name="PowerPoints" display-name="Power Points" value-type="int" />
		<attribute name="Bennies" display-name="Bennies" value-type="int" />
		
		<attribute name="Hindrances" display-name="Hindrances" value-type="list(string)" />
		<attribute name="Edges" display-name="Edges" value-type="list(string)" />
		<attribute name="Equipment" display-name="Equipment" value-type="list(string)" />
		<attribute name="ArmorHead" display-name="Head Armor" value-type="string" />
		<attribute name="ArmorTorso" display-name="Torso Armor" value-type="string" />
		<attribute name="ArmorArms" display-name="Arms Armor" value-type="string" />
		<attribute name="ArmorLegs" display-name="Legs Armor" value-type="string" />
		<attribute name="WTCarried" display-name="Total WT Carried" value-type="string" />
		<attribute name="WTLimit" display-name="Weight Limit" value-type="string" />
		<attribute name="WTPenalty" display-name="Encumbrance Penalty" value-type="string" />
		
		<attribute name="Skills" display-name="Skills" value-type="list(skill)" />
		<attribute name="Powers" display-name="Powers/Trappings" value-type="list(power)" />
		<attribute name="Weapons" display-name="Weapons" value-type="list(weapon)" />
		
		<attribute name="Injuries" display-name="Permanent Injuries" value-type="string" />

		<attribute name="Wealth" display-name="Money" value-type="string" />
		<attribute name="Description" display-name="Appearance" value-type="string" />
		<attribute name="Personality" display-name="Personality" value-type="string" />
		<attribute name="Background" display-name="Background" value-type="string" />
		<attribute name="Notes" display-name="Session and Campaign Notes" value-type="string" />
		<attribute name="DieRolls" display-name="Common Die Rolls" value-type="list(roll)" />
	</attributes>
	
	<character-types>
		<character-type name="PC" display-name="PC" sort-as="pc">
			<attribute name="Name" />
			<attribute name="Player" />
			<attribute name="Origin" />
			<attribute name="Rating" />
			<attribute name="Destination" />
			<attribute name="Agility" />
			<attribute name="Smarts" />
			<attribute name="Strength" />
			<attribute name="Spirit" />
			<attribute name="Vigor" />
			<attribute name="Charisma" />
			<attribute name="Pace" />
			<attribute name="Parry" />
			<attribute name="Toughness" />
			<attribute name="Fatigue" />
			<attribute name="Wounds" />
			<attribute name="PowerPoints" />
			<attribute name="Bennies" />
			<attribute name="Hindrances" />
			<attribute name="Edges" />
			<attribute name="Equipment" />
			<attribute name="ArmorHead" />
			<attribute name="ArmorTorso" />
			<attribute name="ArmorArms" />
			<attribute name="ArmorLegs" />
			<attribute name="WTCarried" />
			<attribute name="WTLimit" />
			<attribute name="WTPenalty" />
			<attribute name="Skills" />
			<attribute name="Powers" />
			<attribute name="Weapons" />
			<attribute name="Injuries" />
			<attribute name="Wealth" />
			<attribute name="Description" />
			<attribute name="Personality" />
			<attribute name="Background" />
			<attribute name="Notes" />
			<attribute name="DieRolls" />
		</character-type>
	</character-types>
	
	<character-sheets>
		<character-sheet name="PC-FULL" type="PC" display-name="Full Sheet" use-for-create="true" use-for-edit="true">
			<file ref="SVG10-PC-FULL.sheet" platform="iphone" />
			<file ref="SVG10-PC-FULL.pad.sheet" platform="ipad" />
		</character-sheet>
	</character-sheets>

	<imports>
	</imports>
	
	<encounter-elements>
		<encounter-element name="encelem" display-name="Not sure what goes here" />
	</encounter-elements>
</ruleset>